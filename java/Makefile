
download:
	mkdir -p lib
	wget -P lib http://archive.apache.org/dist/velocity/engine/1.7/velocity-1.7.jar
	wget -P lib http://archive.apache.org/dist/velocity/engine/1.7/velocity-1.7-dep.jar
	wget -P lib http://archive.apache.org/dist/velocity/engine/1.6.4/velocity-1.6.4.jar
	wget -P lib http://archive.apache.org/dist/velocity/engine/1.6.4/velocity-1.6.4-dep.jar
	wget -P lib http://archive.apache.org/dist/velocity/engine/1.5/velocity-1.5.jar
	wget -P lib http://archive.apache.org/dist/velocity/engine/1.5/velocity-dep-1.5.jar
