<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0"
  metadata-complete="true">

  <servlet>
    <servlet-name>StocksJsp</servlet-name>
    <servlet-class>StocksJspServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StocksJsp</servlet-name>
    <url-pattern>/stocks-jsp</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>StocksJspEscaped</servlet-name>
    <servlet-class>StocksJspEscapedServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StocksJspEscaped</servlet-name>
    <url-pattern>/stocks-jsp-escaped</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>StocksStringBuilder</servlet-name>
    <servlet-class>StocksStringBuilderServlet</servlet-class>
    <!--init-param>
      <param-name>encoding</param-name>
      <param-value>utf-8</param-value>
    </init-param-->
  </servlet>
  <servlet-mapping>
    <servlet-name>StocksStringBuilder</servlet-name>
    <url-pattern>/stocks-stringbuilder</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>StocksVelocity</servlet-name>
    <servlet-class>StocksVelocityServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StocksVelocity</servlet-name>
    <url-pattern>/stocks-velocity</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>StocksStringBuilder</servlet-name>
    <servlet-class>StocksStringBuilderServlet</servlet-class>
    <!--init-param>
      <param-name>encoding</param-name>
      <param-value>utf-8</param-value>
    </init-param-->
  </servlet>
  <servlet-mapping>
    <servlet-name>StocksStringBuilder</servlet-name>
    <url-pattern>/stocks-stringbuilder</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>StocksKwartzite</servlet-name>
    <servlet-class>StocksKwartziteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StocksKwartzite</servlet-name>
    <url-pattern>/stocks-kwartzite</url-pattern>
  </servlet-mapping>

</web-app>
